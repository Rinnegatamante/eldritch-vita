PickaxeDesc = "Tandetnej jakości przyrząd\nsłużące do rozłupywania\nskał i kamieni. Rozpadnie\nsię po paru użyciach."
Ropegun = "Pistolet z Kotwiczką"
RopegunDesc = "Wystrzeliwuje hak z liną,\nktóry przyczepi się do\nkażdej stałej powierzchni."
ClimbingBoots = "Buty Wspinaczkowe"
ClimbingBootsDesc = "Buty te uchronią cię od\nobrażeń od upadku oraz\nspadających sopli lodu."
PowerSummon = "Przywołanie"
StatueSummon = "Oleisty Posąg"
PowerFlare = "Flara"
StatueFlare = "Płonący Posąg"
PlaneKeys = "Klucze Do Samolotu"
Book_MoM = "Czy ta księga była tu już wcześniej?\n\nStronice łopoczą dziko, gdy mroźny wiatr szaleje wokół ciebie.\n\nGóry szaleństwa czekają..."
Book_PlaneNoKeys = "Brakuje kluczy do samolotu. Bez nich nie polecisz."
Book_PlaneWithKeys = "Silnik samolotu dławi się,\ngdy przekręcasz klucz w stacyjce.\n\nStarasz się nie rozmyślać o reliktach\nniemożliwie odległej przeszłości, które\nbyło ci ujrzeć. Gdy samolot odrywa się\nod ziemi, pozwalasz sobie na ostatnie\nspojrzenie na górski szczyt wraz z jej\nnieziemskimi, równobocznymi monolitami\nna widok których wstrząsa tobą dreszcz.\n\nUdało ci się ujść z życiem, ale czy\nze zdrowymi zmysłami? "
Book_CampingKit = "Niezbędnik Obozowy\n\nWytrzymały płócienny worek zawierający\nrozmaite przedmioty przydatne przy\nrozbijaniu obozu.\n\nMając przy sobie Niezbędnik możesz\npodsycać ogniska. Odpoczynek przy\npodsyconym ognisku przywraca\nwięcej zdrowia.\n\nKoszt podsycenia ogniska wynosi \ue0f1 × #{i:CampingKit:StokePriceTag}."
Book_ClimbingBoots = "Buty Wspinaczkowe\n\nStylowa para traperów na kolczastej\npodeszwie.\n\nNosząc te buty nie odnosisz obrażeń od\nupadku a przechodzenie pod soplami lodu\nnie powoduje ich oderwania."
Book_Soapstone = "Steatyt\n\nZielonkawy fragment steatytu w kształcie\ngwiazdy pięcioramiennej. Nie ma\nspecjalnych właściwości."
Book_Summon = "Moc Przywołania\n\nWciśnij #{n:EldritchInput:UsePower} by wezwać niepowstrzymanego shoggotha.\n\nZaatakuje on wszystko w zasięgu wzroku, łącznie z tobą!\n\nKoszt Przywołania wynosi \ue0f1 × #{i:PowerSummon:PowerCost}."
Book_Flare = "Moc Flary\n\nWciśnij #{n:EldritchInput:UsePower} by cisnąć mistyczną flarą.\n\nPrzez jakiś czas rozświetli ci ona drogę, jak też rozproszy uwagę przeciwników.\n\nKoszt Flary wynosi \ue0f1 × #{i:PowerFlare:PowerCost}."
ShoggothShopkeeperTalk = "Shoggoth bełkocze do ciebie niezrozumiale."
SquidmanFriendlyShopkeeperTalk = "Gwiezdny pomiot wita cię serdecznie.\n\n„Witaj, przyjacielu! Czy nie chcesz\nzakupić czegoś w moim skromnym sklepie?”"
